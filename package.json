{
  "name": "array-query",
  "version": "0.2.1",
  "description": "Provides an interface to pull objects out of a JavaScript array with minimal code. Useful for Backbone collections and similar scenarios.",
  "type": "module",
  "author": "Jacob Wright <jacwright@gmail.com>",
  "keywords": [
    "array",
    "database",
    "query",
    "select",
    "where"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/array-query.umd.js",
  "module": "./dist/array-query.es.js",
  "types": "./dist/query.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-lib.js",
      "require": "./dist/my-lib.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "maintainers": [
    {
      "name": "Jacob Wright",
      "email": "jacwright@gmail.com",
      "web": "http://jacwright.com"
    }
  ],
  "contributors": [
    {
      "name": "Jacob Wright",
      "email": "jacwright@gmail.com",
      "web": "http://jacwright.com"
    }
  ],
  "homepage": "https://github.com/jacwright/array-query",
  "repository": {
    "type": "git",
    "url": "git://github.com/jacwright/array-query.git"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "jest": "^29.5.0",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0"
  }
}
